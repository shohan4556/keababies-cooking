{
  "extends": "tslint:recommended",
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "semicolon": false,
    "trailing-comma": false,
    "typedef": [
      true,
      "call-signature",
      "property-declaration",
      "variable-declaration",
      "parameter"
    ]
  }
}
